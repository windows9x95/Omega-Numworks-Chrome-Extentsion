{
  "name": "Omega Extension",
  "description": "All usefulls links related to Omega-Numworks",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "contentSettings"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/Omega-Logo.png"
  },
  "icons": {
    "512": "img/Omega-Logo.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "MacCtrl+Shift+A"
      },
      "description": "Ouvre l'extention Omega"
    }
  }
}